        "platform":                 "Javascript/Node.js",
        "mode":                     "daemon",
        "messagebox":               true,
        "subscribe":                "messagebox",
        "materialize":              true,
        "stopBeforeUpdate":         true,
        "icon":                     "mbus.png",
        "extIcon":                  "https://raw.githubusercontent.com/Apollon77/ioBroker.mbus/master/admin/mbus.png",
